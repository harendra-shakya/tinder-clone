{
  "expo": {
    "name": "starter-rn-expo-tailwind-ts",
    "slug": "starter-rn-expo-tailwind-ts",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./public/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./public/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "googleServicesFile": "./GoogleService-Info.plist"
    },
    "android": {
      "googleServicesFile": "./google-services.json",
      "adaptiveIcon": {
        "foregroundImage": "./public/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.harendrashakya.starterrnexpotailwindts"
    },
    "plugins": ["@react-native-google-signin/google-signin"],
    "web": {
      "favicon": "./public/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "8a614666-6547-4c21-a86c-d313ae3b4964"
      }
    }
  }
}
